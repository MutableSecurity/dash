{
    "editor.tabSize": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true,
        "source.sortMembers": true
    },
    "files.exclude": {
        ".git": true,
        "node_modules": true,
        "yarn.lock": true
    },
    "[dotenv]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    }
}
